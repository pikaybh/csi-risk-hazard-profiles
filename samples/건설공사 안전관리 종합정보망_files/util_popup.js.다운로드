// 사고 코드 검색
function acd_code_pop(title, idx, code_group) {
	wdt = 600;
	hgt = 730;
	xPos = Math.ceil((screen.width - wdt) / 2);
	yPos = Math.ceil((screen.height - hgt) / 2);
	var params = "?code=";
	params += (title != null) ? "&title=" + encodeURIComponent(title) : "";
	params += (idx != null) ? "&idx=" + idx : "";
	params += (code_group != null) ? "&code_group=" + code_group : "";
	window.open("/acd/acdCodePop.do" + params, "acdCodePop", "width=" + wdt + ",height=" + hgt + ",menubar=no,toolbar=no,scrollbars=yes,resizable=yes,left=" + xPos + ",top=" + yPos);
}

//사고 사유 검색
function acd_reason_pop(title, idx, code_group) {
	wdt = 600;
	hgt = 530;
	xPos = Math.ceil((screen.width - wdt) / 2);
	yPos = Math.ceil((screen.height - hgt) / 2);
	window.open("", "acdReasonPop", "width=" + wdt + ",height=" + hgt + ",menubar=no,toolbar=no,status=no,scrollbars=1,resizable=yes,left=" + xPos + ",top=" + yPos);
	jQuery("<form target='acdReasonPop' method='post' action='/acd/acdReasonPop.do'>" +
		   "<input type='hidden' name='code' value='' >" +	
		   "<input type='hidden' name='title' value='"+title+"' >" +
		   "<input type='hidden' name='idx' value='"+idx+"' >" +
		   "<input type='hidden' name='code_group' value='"+code_group+"' >" +
		   "<input type='hidden' name='data02' value='Y' >" +		   
		   "</form>").appendTo('body').submit().remove();
}

