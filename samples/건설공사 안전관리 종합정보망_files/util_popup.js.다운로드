// 리포트 뷰어
// purpose_cd : 직인코드(seal_purpose)
// viewType : PDF -> viewer.hideToolbarItem(['ppt', 'xls', 'hwp', 'doc']);
// 유형 추가시, 타입 추가하여 reportViewPop JSP에 분기 추가.
function go_rpt_viewer(report_head, report_id, params, purpose_cd, const_nm, markany_yn, viewType) {
	wdt = 1024;
	hgt = screen.height - 100;
	xPos = Math.ceil((screen.width - wdt) / 2);
	yPos = Math.ceil((screen.height - hgt) / 2) - 50;
	var tmpParam = "";
	console.log("go_rpt_viewer",markany_yn);
	if(markany_yn == "Y"){
		tmpParam = "markany_yn=Y&";
	}
	
	if(viewType != undefined){
		tmpParam += "viewType="+viewType+"&";
	}

	if (params.indexOf("[cts_report]") > -1) {
		tmpParam += "cts_report=Y&";
	}

	window.open("/com/reportViewPop.do?" + tmpParam + "report_head=" + encodeURIComponent(report_head) + "&report_id=" + report_id + "&params=" + encodeURIComponent(params), "rptViewer", "width=" + wdt + ",height=" + hgt + ",menubar=no,toolbar=no,scrollbars=yes,resizable=yes,left=" + xPos + ",top=" + yPos);
	
	if(purpose_cd != undefined){
		//리포트 뷰어 실행 시 로그 insert
		var callurl = '/com/insertSealPrintLogAjax.do';
		var jsonData = { purpose_cd: purpose_cd , const_nm: const_nm};
		ajaxJson(callurl, jsonData, successFunc, errFunc);
		
		function successFunc(data) {
			if(data.isOk != 'Y'){
				alert(data.errMsg);
			}
		}
		function errFunc() {
			alert('저장 처리 중, 에러가 발생하였습니다.');
		};
	}
}

// 수준평가 설명회 명찰 인쇄
function go_levNameCard_rpt_viewer(report_head, report_id, params,event_seq,user_id) {
	wdt = 1024;
	hgt = screen.height - 100;
	xPos = Math.ceil((screen.width - wdt) / 2);
	yPos = Math.ceil((screen.height - hgt) / 2) - 50;
	var tmpParam = "";
	
	// if(markany_yn == "Y"){
	// 	tmpParam = "markany_yn=Y&";
	// }

	// if (params.indexOf("[cts_report]") > -1) {
	// 	tmpParam += "cts_report=Y&";
	// }

	window.open("/com/reportViewPop.do?" + tmpParam + "report_head=" + encodeURIComponent(report_head) + "&report_id=" + report_id + "&params=" + encodeURIComponent(params), "rptViewer", "width=" + wdt + ",height=" + hgt + ",menubar=no,toolbar=no,scrollbars=yes,resizable=yes,left=" + xPos + ",top=" + yPos);
	
		//명찰 인쇄시 참석여부 Y
		var callurl = '/lev/nameCradPrint.do';
		var jsonData = {event_seq: event_seq ,
			            user_id: user_id ,
						attend_yn:'Y',
					};
		ajaxJson(callurl, jsonData, successFunc, errFunc);
		
		function successFunc(data) {
			if(data.isOk != 'Y'){
				alert(data.errMsg);
			}
		}
		
		function errFunc() {
			alert('저장 처리 중, 에러가 발생하였습니다.');
		};
	
	
}


