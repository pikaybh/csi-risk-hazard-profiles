function go_smp_rpt_viewer(report_head, report_id, params, const_nm) {
	wdt = 1024;
	hgt = screen.height - 100;
	xPos = Math.ceil((screen.width - wdt) / 2);
	yPos = Math.ceil((screen.height - hgt) / 2) - 50;
	window.open("/com/smpReportViewPop.do?report_head=" + encodeURIComponent(report_head) + "&report_id=" + report_id + "&params=" + encodeURIComponent(params), "rptViewer", "width=" + wdt + ",height=" + hgt + ",menubar=no,toolbar=no,scrollbars=yes,resizable=yes,left=" + xPos + ",top=" + yPos);

	var purpose_cd = null;
	switch (report_id) {
		case 'SMP_SA':
			purpose_cd = 'SA';
			break;
		case 'SMP_SB':
			purpose_cd = 'SB';
			break;
		case 'SMP_SC':
			purpose_cd = 'SC';
			break;
		case 'SMP_SD':
			purpose_cd = 'SD';
			break;
		case 'SMP_SF':
			purpose_cd = 'SF';
			break;
		default:
			purpose_cd = null;
			break;
	}

	// purpose_cd : 직인코드(seal_purpose)
	if(purpose_cd != null){
		//리포트 뷰어 실행 시 로그 insert
		var callurl = '/com/insertSealPrintLogAjax.do';
		var jsonData = {purpose_cd: purpose_cd, const_nm: const_nm};
		ajaxJson(callurl, jsonData, successFunc, errFunc);

		function successFunc(data) {
			if(data.isOk != 'Y'){
				alert(data.errMsg);
			}
		}
		function errFunc() {
			alert('저장 처리 중, 에러가 발생하였습니다.');
		};
	}
}

