// 사고사례보고서
function go_acd_report(biz_id, sago_no) {
	wdt = 840; hgt = 750;
   	xPos = Math.ceil((screen.width - wdt) / 2);
    yPos = Math.ceil((screen.height- hgt) / 2) - 50;
    var pageUrl = '';
    if(biz_id == 'acd21001') {
    	pageUrl = '/accident/acd10.do?method=acd01002_select';
    }else if(biz_id == 'acd41001') {
    	pageUrl = '/accident/acd10.do?method=acd41001_list';
    }
    pageUrl += '&sago_no='+sago_no + '&biz_id='+biz_id + '&popup_yn=Y';
