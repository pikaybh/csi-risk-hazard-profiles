
// 달력 팝업
function Calendar_P(obj) {
	ReturnValue = window.window.showModalDialog("/com/calendar/calendarFrame.jsp", obj.value,
          										"dialogWidth:280px; dialogHeight:330px; edge:sunken; center:yes; resizable:no; status:no; help:no; scroll:no;");
    obj.value = ReturnValue == null || ReturnValue == 'undefined' ? '' : ReturnValue;
    Calendar_Post();
}

// 우편번호 찾기
function find_zip_no(params, version) {
	wdt = 540; hgt = 600;
	xPos = Math.ceil((screen.width - wdt) / 2);
	yPos = Math.ceil((screen.height - hgt) / 2);
	if(version == 'old') {
		window.open("/com/other.do?method=zipFinder"+params, "zipFind", "width="+wdt+",height="+hgt+",menubar=no,toolbar=no,scrollbars=yes,resizable=yes,left="+xPos+",top="+yPos);
	}else {
		window.open("/com/redirect.do?reqUrl=com/popup/jusoLink"+params, "zipFind", "width=" + wdt + ",height=" + hgt + ",menubar=no,toolbar=no,scrollbars=yes,resizable=yes,left=" + xPos + ",top=" + yPos);
	}
}

// 주소찾기 (지번, 도로명주소)
function find_juso(addr, category, opt, mastAddr) {
	/*
		결과 선택 값의 Return은 find_juso_callback(data, opt) 으로 됨
		 - data의 구조
			"id": "주소ID",
			"address": {
				"zipcode": "우편번호",
				"category": "parcel", // parcel : 지번주소, road : 도로명주소
				"road": "도로명주소",
				"parcel": "지번주소",
				"sido": "시도",
				"gugun": "시도",
				"dong": "시도",
				"detail": "시도"
			},
			"point": {
				"x": "127.480785325014",
				"y": "36.116186321055"
			}
	*/

	wdt = 604; hgt = 606;
	xPos = Math.ceil((screen.width - wdt) / 2);
	yPos = Math.ceil((screen.height - hgt) / 2);
	var params = "";
	params += (addr != null) ? "&query=" + encodeURIComponent(addr) : "";
	params += (category != null) ? "&category=" + category : "";
	params += (opt != null) ? "&opt=" + opt : "";
	params += (mastAddr != null) ? "&mast_addr=" + mastAddr : "";
	window.open("/com/vworldJusoPop.do?wdt=" + wdt + params, "jusoList", "width=" + wdt + ",height=" + hgt + ",menubar=no,toolbar=no,status=no,scrollbars=yes,resizable=yes,left=" + xPos + ",top=" + yPos);

	//find_juso_callback
}

