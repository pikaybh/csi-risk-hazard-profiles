// 파일첨부
function go_attach_file(pathKey, subDir, ref_table, ref_pk, params) {
	wdt = 500; hgt = 400;
	xPos = Math.ceil((screen.width - wdt) / 2);
	yPos = Math.ceil((screen.height - hgt) / 2);
	var theURL = '/com/other.do?method=fileAttachPop&pathKey='+pathKey+'.pds.dir&subDir='+subDir+'&ref_table='+ref_table+'&ref_pk='+ref_pk+params;
	window.open(theURL, "fileAttach", "width=" + wdt + ",height=" + hgt + ",menubar=no,toolbar=no,scrollbars=yes,resizable=yes,left=" + xPos + ",top=" + yPos);
}

//CSI : 파일 다운로드
function file_download(downForm, file_no) {
	downForm.file_no.value = file_no;
	downForm.action = '/com/fileDownloadProc.do';
	downForm.target = '_self';
	downForm.submit();
}

//CSI : 파일 다운로드(디코딩이 필요없는 file_no)
function file_downloadByNo(downForm, file_no) {
	downForm.file_no.value = file_no;
	downForm.action = '/com/fileDownloadByNoProc.do';
	downForm.target = '_self';
	downForm.submit();
}

//SANJE 파일 다운로드
function sanje_file_download(downForm, reg_no) {
	downForm.reg_no.value = reg_no;
	downForm.action = '/com/sanjeFileDownloadProc.do';
	downForm.target = '_self';
	downForm.submit();
}

//수준평가 평가항목 템플릿 다운로드
function lev_file_download(downForm, sfile_nm, file_nm) {
	if(file_nm == undefined) { file_nm = sfile_nm.substring(sfile_nm.lastIndexOf('_')+1); }
	downForm.sfile_nm.value = sfile_nm;
	downForm.file_nm.value = file_nm;
	downForm.action = '/com/levFileDownloadProc.do';
	downForm.submit();
}

/*사이냅문서뷰어 미리보기*/
function fn_preview(downForm, file_no){
	var openWindow = window.open('about:blank', file_no);
	downForm.file_no.value = file_no;
	downForm.action = '/com/previewProc.do';
	downForm.target = file_no;
	downForm.submit();
}

/*사이냅문서뷰어 미리보기(디코딩이 필요없는 file_no)*/
function fn_previewByNo(downForm, file_no){
	var openWindow = window.open('about:blank', file_no);
	downForm.file_no.value = file_no;
	downForm.action = '/com/previewByNoProc.do';
	downForm.target = file_no;
	downForm.submit();
}

