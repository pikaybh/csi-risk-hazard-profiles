//그룹웨어 연동 팝업창 띄우기(key:input names ; code1: DFS,SMP)
function go_gw(key,code1) {
	//if(browser() == "IE") {
	wdt = 1024; hgt = 800;
	xPos = Math.ceil((screen.width - wdt) / 2);
	yPos = Math.ceil((screen.height - hgt) / 2);
	//window.open("/gw/gwAcptListMcrPop.do?key="+key+"&code1="+code1, "gwMain", "width=" + wdt + ",height=" + hgt + ",menubar=no,toolbar=no,scrollbars=yes,resizable=yes,left=" + xPos + ",top=" + yPos);
	window.open("", "gwMain", "width=" + wdt + ",height=" + hgt + ",menubar=no,toolbar=no,scrollbars=yes,resizable=yes,left=" + xPos + ",top=" + yPos);
	$('#form_gwPop > input[name=key]').val(key);
	$('#form_gwPop > input[name=code1]').val(code1);

	// $('#form_gwPop').attr('action','/gw/gwAcptListMcrPop.do');
	if (code1 == "SMP") {
		$('#form_gwPop').attr('action','/gw/gwSendListMcrPop.do');
	} else {
		$('#form_gwPop').attr('action','/gw/gwAcptListMcrPop.do');
	}
	$('#form_gwPop').attr('target','gwMain');
	$('#form_gwPop').submit();
	$('#form_gwPop').attr('target','_self');
	/*}else{
		alert("그룹웨어 기능은 Internet Explorer 브라우저에서만 사용 가능합니다.\nInternet Explorer 브라우저로 다시 접속해 주십시오.");
	}*/
}

//post 팝업 열기
//통합메세지 팝업창
function sendMsgPop(inst_nm, inst_chrgr_nm, hp_no, inst_chrgr_id) {
	wdt = 800; hgt = 800;
	xPos = Math.ceil((screen.width - wdt) / 2);
	yPos = Math.ceil((screen.height - hgt) / 2);
	window.open("", "sendMsgPop", "width="+wdt+",height="+hgt+",menubar=no,toolbar=no,scrollbars=yes,resizable=yes,left="+xPos+",top="+yPos);
	
	$('#form_sendMsgPop > input[name=inst_nm]').val(inst_nm);
	$('#form_sendMsgPop > input[name=inst_chrgr_nm]').val(inst_chrgr_nm);
	$('#form_sendMsgPop > input[name=hp_no]').val(hp_no);
	$('#form_sendMsgPop > input[name=inst_chrgr_id]').val(inst_chrgr_id);
	
	$('#form_sendMsgPop').attr('action','/com/sendMsgPop.do');
	$('#form_sendMsgPop').attr('target','sendMsgPop');
	$('#form_sendMsgPop').submit();
	$('#form_sendMsgPop').attr('target','_self');
}

