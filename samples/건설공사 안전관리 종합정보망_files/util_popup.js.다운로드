//공사정보 찾기
function find_constInfo(isAcdEdit) {

	if(isAcdEdit == null|| isAcdEdit == undefined  || isAcdEdit == "" || isAcdEdit == "null" || isAcdEdit == "undefined" || (isAcdEdit != "Y" && isAcdEdit != "N" ) ){
		isAcdEdit = "N";
	}
	
	wdt = 1024; hgt = 700;
	xPos = Math.ceil((screen.width - wdt) / 2);
	yPos = Math.ceil((screen.height - hgt) / 2);
	window.open("/acd/searchConstInfoPop.do?isAcdEdit="+isAcdEdit, "smp2cws", "width=" + wdt + ",height=" + hgt + ",menubar=no,toolbar=no,scrollbars=yes,resizable=yes,status=no,left=" + xPos + ",top=" + yPos);
}

